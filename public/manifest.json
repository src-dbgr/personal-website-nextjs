{
  "id": "https://sblm.netlify.app/",
  "name": "Samuel B",
  "short_name": "SBLM",
  "start_url": "/",
  "scope": "/",
  "display": "standalone",
  "display_override": ["standalone", "fullscreen"],
  "theme_color": "#3d8b68",
  "background_color": "#b5aba6",
  "orientation": "portrait",
  "icons": [
    {
      "src": "/assets/images/icons/icon-512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/assets/images/icons/android-chrome-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/assets/images/icons/android-chrome-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    },
    {
      "src": "/assets/images/icons/apple-touch-icon.png",
      "sizes": "180x180",
      "type": "image/png"
    },
    {
      "src": "/assets/images/icons/favicon-32x32.png",
      "sizes": "32x32",
      "type": "image/png"
    },
    {
      "src": "/assets/images/icons/favicon-16x16.png",
      "sizes": "16x16",
      "type": "image/png"
    }
  ],
  "screenshots": [
    {
      "src": "/assets/images/screenshots/sb-screenshot.png",
      "sizes": "1280x720",
      "type": "image/png",
      "label": "Homescreen of Samuel IT",
      "form_factor": "wide"
    },
    {
      "src": "/assets/images/screenshots/sb-screenshot-portrait.png",
      "sizes": "720x1280",
      "type": "image/png",
      "label": "Portrait Homescreen of Samuel IT"
    }
  ],
  "categories": ["education", "productivity"],
  "protocol_handlers": [
    {
      "protocol": "mailto",
      "url": "/newEmail?to=%s"
    }
  ],
  "shortcuts": [
    {
      "name": "New Project",
      "url": "/new-project",
      "icons": [
        {
          "src": "/assets/images/icons/fav.png",
          "sizes": "2000x2000",
          "type": "image/png"
        }
      ]
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "application/x-www-form-urlencoded",
    "params": {
      "title": "name",
      "text": "description",
      "url": "link"
    }
  }
}
